#: ../../source/box.rst:14
msgid "``Layer``: the combobox will display all the vector layers loaded in QGIS"
msgstr ""

#: ../../source/box.rst:16
msgid "``Grouping Field (Optional)``: the grouping field. If no field is added, a generic box plot for the Y value will be displayed"
msgstr ""

#: ../../source/box.rst:19
msgid "``Y Field``: the Y field"
msgstr ""

#: ../../source/box.rst:21
msgid "``Marker Color``: color of the box"
msgstr ""

#: ../../source/box.rst:23
msgid "``Stroke Color``: border color"
msgstr ""

#: ../../source/box.rst:25
msgid "``Stroke Width``: the width of the border"
msgstr ""

#: ../../source/box.rst:27
msgid "``Transparency``: transparency level of the box"
msgstr ""

#: ../../source/box.rst:30
msgid "Plot Customizations"
msgstr ""

#: ../../source/box.rst:31
msgid "``Show Legend``: show the legend of the current plot"
msgstr ""

#: ../../source/box.rst:33
msgid "``Horizontal Legend``: check if you want to have an horizontal legend"
msgstr ""

#: ../../source/box.rst:35
msgid "``Plot Title``: the plot title"
msgstr ""

#: ../../source/box.rst:37
msgid "``Legend Title``: the title of the legend"
msgstr ""

#: ../../source/box.rst:39
msgid "``X Label``: X label text"
msgstr ""

#: ../../source/box.rst:41
msgid "``Y Label``: Y Label text"
msgstr ""

#: ../../source/box.rst:43
msgid "``Invert X Axis``: check if you want to invert the ordering of the x axis"
msgstr ""

#: ../../source/box.rst:45
msgid "``X Axis Mode``: choose between Linear, Logarithmic or Categorized"
msgstr ""

#: ../../source/box.rst:47
msgid "``Invert Y Axis``: check if you want to invert the ordering of the y axis"
msgstr ""

#: ../../source/box.rst:49
msgid "``Y Axis Mode``: choose between Linear, Logarithmic or Categorized"
msgstr ""

#: ../../source/box.rst:51
msgid "``Box Orientation``: the orientation of the box, either *Horizontal* or *Vertical*"
msgstr ""

